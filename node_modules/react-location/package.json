{
  "name": "react-location",
  "version": "2.0.7",
  "description": "⚛️ React utilities for location routing, params, queries, and hashes.",
  "author": "tannerlinsley",
  "license": "MIT",
  "repository": "tannerlinsley/react-location",
  "main": "dist/index.js",
  "_module": "dist/index.es.js",
  "_jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "is-ci \"yarn test:coverage\" \"yarn test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "precommit": "lint-staged",
    "validate": "npm run test:coverage && npm run test:e2e:run",
    "format": "prettier \"**/*.+(js|json|less|css|ts|tsx|md)\" --write",
    "lint": "jest --config test/jest.lint.js",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn build",
    "release": "yarn publish",
    "releaseNext": "yarn publish --tag next",
    "setup": "npm install && npm run validate"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^16.8.6"
  },
  "dependencies": {
    "qss": "^2.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@svgr/rollup": "^4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "cross-env": "^5.1.4",
    "eslint": "5.x",
    "eslint-config-kentcdodds": "^14.3.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^4.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "eslint-plugin-standard": "^4.0.0",
    "is-ci": "^2.0.0",
    "jest": "^24.4.0",
    "jest-runner-eslint": "^0.7.3",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.2.1",
    "react-testing-library": "^6.0.0",
    "rollup": "^0.68.0",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0"
  },
  "files": [
    "dist"
  ]
}
